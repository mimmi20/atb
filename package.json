{
  "name": "atb",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:all": "concurrently \"tsc && vite\" \"php -S 0.0.0.0:8082 -t public/ public/index.php -q -ddisplay_errors=1\"",
    "dev": "tsc && vite",
    "build": "tsc && vite build",
    "preview": "tsc && vite preview",
    "test": "tsc && vite build && vitest run",
    "coverage": "tsc && vite build && vitest run --coverage",
    "typecheck": "tsc && vitest typecheck",
    "prettier": "prettier --config prettier.config.mjs --check public/**/*.ts",
    "prettier-fix": "prettier --config prettier.config.mjs --write public/**/*.ts",
    "stylelint": "stylelint \"public/css/*.css\" --color --cache",
    "stylelint-fix": "stylelint \"public/css/*.css\" --color --cache --fix",
    "eslint": "eslint . --config eslint.config.mjs",
    "eslint-fix": "eslint . --config eslint.config.mjs --fix",
    "oxlint": "oxlint .",
    "oxlint-fix": "oxlint . --fix",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "cypress:run:chromium": "cypress run --browser chromium",
    "test:e2e": "start-server-and-test 'php -S localhost:8082 -t public/ public/index.php -q -ddisplay_errors=1' http://localhost:8082 'npx cypress run'"
  },
